<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/material_SN"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="SN:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <TextView
            android:id="@+id/material_FEPO"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="FEPO:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <TextView
            android:id="@+id/material_QuantityPs"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="总码长:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <TextView
            android:id="@+id/material_QuantityPsCheck"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="已传码长:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <!--<TextView-->
            <!--android:id="@+id/material_QuantityPsUnCheck"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="80dp"-->
            <!--android:text="未传码长:"-->
            <!--android:layout_weight="1"-->
            <!--android:gravity="center"-->
            <!--android:textSize="10sp"-->
            <!--android:textColor="@color/black"/>-->
        <TextView
            android:id="@+id/material_TotalNum"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="总匹数:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <Button
            android:id="@+id/material_CheckedNum"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="已传匹数:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp" />
        <!--<Button-->
            <!--android:id="@+id/material_UnCheckedNum"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="80dp"-->
            <!--android:text="未传匹数:"-->
            <!--android:layout_weight="1"-->
            <!--android:gravity="center"-->
            <!--android:textSize="10sp"/>-->
        <TextView
            android:id="@+id/material_nowMC"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="本次扫描码长:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <Button
            android:id="@+id/material_nowPS"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="本次扫描匹数:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"/>
        <TextView
            android:id="@+id/material_isMatch"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="匹配erp:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
        <Button
            android:id="@+id/material_NoNo"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="未传待扫:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp" />
        <TextView
            android:id="@+id/material_exemption"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="免检状态:"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"
            android:textColor="@color/black"/>
    </LinearLayout>

</LinearLayout>