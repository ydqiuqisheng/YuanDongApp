<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/check_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="收料日期"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/info_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="资料日期"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/vat_no"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="缸号"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/PNo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="匹号"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1.1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/FLaw"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="瑕疵"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/clothLevel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="等级"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1.1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/actualWidth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="实际门幅"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/standardWidth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="标准门幅"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/remark1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="布面情况备注1"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"/>
        <include layout="@layout/line_vertical"/>
        <TextView
            android:id="@+id/remark2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="布面情况备注2"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="10sp"/>
    </LinearLayout>
    <include layout="@layout/line"/>
</LinearLayout>